    <pattern> 1) </pattern>
    <replacement> -1) </replacement>
  </replace>
  <replace>
    <pattern> 2) </pattern>
    <replacement> -2) </replacement>
  </replace>
  <replace>
    <pattern> 3) </pattern>
    <replacement> -3) </replacement>
  </replace>
  <replace>
    <pattern> 4) </pattern>
    <replacement> -4) </replacement>
  </replace>
  <replace>
    <pattern> 5) </pattern>
    <replacement> -5) </replacement>
  </replace>
  <replace>
    <pattern> 6) </pattern>
    <replacement> -6) </replacement>
  </replace>
  <replace>
    <pattern> 7) </pattern>
    <replacement> -7) </replacement>
  </replace>
  <replace>
    <pattern> 8) </pattern>
    <replacement> -8) </replacement>
  </replace>
  <replace>
    <pattern> 9) </pattern>
    <replacement> -9) </replacement>
  </replace>
  <replace>
    <pattern> a) </pattern>
    <replacement> -a) </replacement>
  </replace>
  <replace>
    <pattern> b) </pattern>
    <replacement> -b) </replacement>
  </replace>
  <replace>
    <pattern> c) </pattern>
    <replacement> -c) </replacement>
  </replace>
  <replace>
    <pattern> d) </pattern>
